<body style="margin: 0px auto;max-width: 500px;margin-top: 60px">


<div class="container">


    <h2 class="h4 mb-5 font-weight-bold" style="text-align: center;background: #141313;color: white; height: 50px;padding-top: 7px" >Add a Client</h2>
    <form action="/addClientNew" method="post">

        <div class="form-group">
            <input type="text" class="form-control" placeholder="Fullname" name="fullnameClientAdd">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Address" name="addressClientAdd">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Number Phone" name="numberphoneClientAdd">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Password" name="passClientAdd">
        </div>
<!--        <div class="form-group">-->
<!--            <input type="file" name="avatar2">-->
<!--        </div>-->
        <div class="form-group" style="text-align: center">
            <input type="submit" value="Add" class="btn btn-primary py-3 px-5">
        </div>
    </form>



</div>




<!-- loader -->
<div id="ftco-loader" class="show fullscreen">
    <svg class="circular" width="48px" height="48px">
        <circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/>
        <circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10"
                stroke="#F96D00"/>
    </svg>
</div>


